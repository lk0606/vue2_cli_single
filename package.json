{
  "name": "vue2_cli_single",
  "version": "1.0.0",
  "description": "a vue-cli for single page application",
  "main": "webpack.config.js",
  "scripts": {
    "test": "node test/smoke/index.js",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod1.js",
    "build:stats": "cross-env NODE_ENV=production webpack --config build/webpack.prod1.js --json > stats.json",
    "b:ssr": "cross-env RENDER_MODE=server webpack --config build/webpack.ssr1.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev1.js",
    "watch": "webpack --watch",
    "cls:m": "rm -rf dist",
    "cls:wf": "cd dist && del *.js && del *.css && del *.png",
    "cls:ww": "cd dist && del bundle.js",
    "cls": "rm -rf dist || rd dist /S /Q",
    "cls:d": "rimraf ./dist",
    "dll": "webpack --config build/webpack.dll.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lk0606/vue2_cli_single.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lk0606/vue2_cli_single/issues"
  },
  "homepage": "https://github.com/lk0606/vue2_cli_single#readme",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "autoprefixer": "^9.6.5",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "express": "^4.17.1",
    "file-loader": "^4.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "glob": "^7.1.5",
    "html-webpack-externals-plugin": "^3.8.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "px2rem-loader": "^0.1.9",
    "raw-loader": "^0.5.1",
    "rimraf": "^3.0.0",
    "shelljs": "^0.8.3",
    "speed-measure-webpack-plugin": "^1.3.1",
