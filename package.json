{
  "name": "vue2_cli_single",
  "version": "1.0.0",
  "description": "a vue-cli for single page application",
  "main": "webpack.config.js",
  "scripts": {
    "build": "cross-env webpack --config webpack.prod.js",
    "dev": "webpack-dev-server --config webpack.dev.js",
    "watch": "webpack --watch",
    "cls:m": "rm -rf dist",
    "cls:wf": "cd dist && del *.js && del *.css && del *.png",
    "cls:ww": "cd dist && del bundle.js",
    "cls": "rm -rf dist || rd dist /S /Q",
    "cls:d": "rimraf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lk0606/vue2_cli_single.git"
  },
